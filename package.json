{
  "name": "frontend-test-react",
  "version": "1.0.0",
  "description": "Technical test for Frontend Engineer position at Aircall",
  "main": "index.jsx",
  "repository": "https://github.com/aircall/frontend-test-react.git",
  "author": "Xavier Durand <jobs@aircall.io>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "NODE_ENV=production webpack --mode production",
    "lint": "./node_modules/.bin/eslint  ./src -f table --ext .js --ext .jsx || true",
    "lint-fix": "./node_modules/.bin/eslint --fix ./src -f table --ext .js --ext .jsx",
    "precommit": "pretty-quick --staged",
    "test": "./node_modules/mocha/bin/mocha --opts ./mocha.opts ./src/**/__tests__/*.test.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "aigle": "^1.13.0-alpha.12",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "2.1.*",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "expect": "^24.7.1",
    "html-loader": "0.5.*",
    "html-webpack-plugin": "3.2.*",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^6.0.2",
    "nock": "^10.0.6",
    "prettier": "^1.16.4",
    "prettier-eslint-cli": "^4.7.1",
    "pretty-quick": "^1.10.0",
    "redux-mock-store": "^1.5.3",
    "style-loader": "0.23.*",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "4.29.*",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "3.3.*",
    "webpack-dev-server": "3.2.*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
