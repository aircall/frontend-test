{
    "extends": "airbnb",
    "rules": {
        "react/forbid-prop-types": [ "off" ],
        "react/no-unused-prop-types": [ "off" ],
        "react/no-unused-state": [ "off" ],
        "react/prop-types": [ "off" ],
        "no-useless-constructor": [ "off" ]
    },
    "env": {
        "browser": true,
        "jest": true
    }
}